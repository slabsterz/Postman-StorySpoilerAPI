		{
			"name": "Search Storyspoiler by Title",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "f85ee1f1-f9a2-4213-9bee-527c9e3b97dd",
						"exec": [
							"pm.test(\"Response status should be 200\", function() {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const responseJson = pm.response.json();\r",
							"\r",
							"pm.test(\"Response should be a non empty array\", function() {\r",
							"    pm.expect(responseJson).to.be.an(\"array\");\r",
							"    pm.expect(responseJson.length).to.be.greaterThan(0);\r",
							"});\r",
							"\r",
							"const storyTitle = pm.collectionVariables.get(\"randomTitle\");\r",
							"const foundStory = responseJson[0];\r",
							"\r",
							"pm.test(\"Response should contain matching story title\", function() {\r",
							"    pm.expect(foundStory).to.not.be.null;\r",
							"    pm.expect(foundStory.title).to.be.equal(storyTitle);\r",
							"});"
