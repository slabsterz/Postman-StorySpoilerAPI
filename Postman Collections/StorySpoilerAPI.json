		{
			"name": "Create New Story Spoiler",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "a4b26f6f-7869-4e62-b5fc-243e992ca578",
						"exec": [
							"const randomNumber = Math.floor(Math.random() * 1000);\r",
							"const randomTitle = `Storyspoiler${randomNumber}`;\r",
							"\r",
							"pm.collectionVariables.set(\"randomTitle\", randomTitle);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "1d3fea1a-aab2-412f-b279-04696ec153e9",
						"exec": [
							"pm.test(\"Response code should be 201\", function() {\r",
							"    pm.response.to.have.status(201);\r",
							"});\r",
							"\r",
							"const creationMessage = \"Successfully created!\";\r",
							"const responseJson = pm.response.json();\r",
							"\r",
							"pm.test(\"Response message should be 'Successfully created!\", function() {\r",
							"    pm.expect(responseJson.msg).to.be.equal(creationMessage);\r",
							"});\r",
							"\r",
							"const storyId = responseJson.storyId;\r",
							"pm.collectionVariables.set(\"storyId\", storyId);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"id": "ab1e55a1-e9c3-456f-a6e9-a13abefe2d22",
