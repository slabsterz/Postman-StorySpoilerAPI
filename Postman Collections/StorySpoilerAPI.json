		{
			"name": "Login and Authenticate",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "c9fa9e3c-309b-4831-b670-4f9b5ba00b50",
						"exec": [
							"pm.test(\"Response status should be 200\", function() {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"const reponseJson = pm.response.json();\r",
							"\r",
							"pm.test(\"Response object should contain username, password and accessToken properties\", function() {\r",
							"    pm.expect(reponseJson).to.be.an(\"object\");\r",
							"    pm.expect(reponseJson).to.haveOwnProperty(\"username\");\r",
							"    pm.expect(reponseJson).to.haveOwnProperty(\"password\");\r",
							"    pm.expect(reponseJson).to.haveOwnProperty(\"accessToken\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response property 'accessToken' should not be empty\", function() {\r",
							"    pm.expect(reponseJson.accessToken).to.not.be.empty;\r",
							"});\r",
							"\r",
							"pm.collectionVariables.set(\"accessToken\", reponseJson.accessToken);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "558c333b-70d4-4b38-bca7-df34fbc1847a",
